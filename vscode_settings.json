{
    "editor.renderWhitespace": "boundary",
    "files.trimTrailingWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.pyc":true,
        "**/.un~":true
    }
    ,
    "window.zoomLevel": 0,
    "editor.wordWrap": true,
    "editor.rulers": [80],
    "rfLanguageServer.libraries": [
        "BuiltIn-3.0.4",
        "SeleniumLibrary-3.2.0",
        "Dialogs-3.1.1",
        "Collections-3.1.1"
    ],
}
